{
  "files.exclude": {
    "**/apps/web-e2e": false,
    "**/apps/**/.browserslistrc": false,
    "**/apps/**/assets": false,
    "**/apps/**/environments": false,
    "**/apps/web/src/main.ts": false,
    "**/apps/web/src/favicon*": false,
    "**/apps/*/README.md": false,
    "**/libs/*/README.md": false,
    "**/polyfills.ts": false,
    "**/test-setup*": false,
    "**/.eslint*": false,
    "**/jest*": false,
    "**/tsconfig*": false,
    "**/coverage/": false,
    "**/dist/": false,
    "**/screenshots/": false,
    "**/tools/": false,
    "**/.editorconfig": false,
    "**/.gitignore": false,
    "**/.gitkeep": false,
    "**/.npmrc": false,
    "**/.prettierignore": false,
    "**/.prettierrc": false,
    "**/decorate-angular-cli.js": false,
    "**/FUNDING.yml": false,
    "**/LICENSE": false,
    "**/readme-nx.md": false,
    "**/node_modules": false
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80
  ],
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "angular_ngrx",
  "material-icon-theme.folders.associations": {
    "web": "client",
    "web-e2e": "test",
    "options": "config",
    "directives": "components",
    "enums": "mjml",
    "models": "mjml",
    "forms": "docs",
    "wrappers": "hook",
    "document": "",
    "workflows": "github",
    "formly": "scala",
  },
  "material-icon-theme.files.associations": {
    "*.interceptor.ts": "apollo",
    "*.events.ts": "tcl",
    "*.data.ts": "database",
    "*.model.ts": "mjml",
    "*.enum.ts": "database",
    "*.const.ts": "database",
    "model.ts": "mjml",
    "index.ts": "url",
    "public-api.ts": "url",
    "models.ts": "url",
    "services.ts": "url",
  },
  "cSpell.words": [
    "contenteditable",
    "devkit",
    "durak",
    "exitzero",
    "fontawesome",
    "FORMLY",
    "ngcc",
    "nrwl",
    "statvoo",
    "tailwindcss"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "svg.preview.background": "transparent",
}
